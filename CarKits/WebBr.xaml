<Window x:Class="CarKits.WebBr"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CarKits"
        mc:Ignorable="d"
        Title="WebBr" Height="500" Width="700" Loaded="Window_Loaded">
    <Grid>
        <WebBrowser x:Name="myWeb"  HorizontalAlignment="Left" Height="399" Margin="10,60,0,0" VerticalAlignment="Top" Width="672"/>
        <TextBox x:Name="search" HorizontalAlignment="Left" Height="31" Margin="240,10,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="348"/>
        <Button x:Name="button" Content="Вперед" HorizontalAlignment="Left" Height="31" Margin="137,10,0,0" VerticalAlignment="Top" Width="60" Click="button_Click"/>
        <Button x:Name="button1" Content="Назад" HorizontalAlignment="Left" Height="31" Margin="35,10,0,0" VerticalAlignment="Top" Width="60" Click="button1_Click"/>
        <Button x:Name="button2" Click="button2_click" Content="Поиск" HorizontalAlignment="Left" Height="31" Margin="610,10,0,0" VerticalAlignment="Top" Width="72"/>

    </Grid>
</Window>
